{
  "testMatch": ["<rootDir>/test/**/*.test.js"],
  "transform": {},
  "collectCoverage": true,
  "collectCoverageFrom": ["<rootDir>/app/**/*.js"],
  "coverageDirectory": "coverage",
  "coverageReporters": ["text-summary", "lcov"],
  "verbose": true,
  "resetMocks": true,
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  }
}
